include mobnet
include stp
include virtual_machines

Host *
    ServerAliveInterval 60
    SetEnv TERM=xterm

Host wert.dk
    HostName wert.dk
    Port 23432
    User ask
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal
 
Host zenbook
    HostName 192.168.0.107
    User ges
    Port 22
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/personal

Host zenbook-win10
    HostName 172.31.1.242
    User ges
    Port 22
    ProxyJump zenbook


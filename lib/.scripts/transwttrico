#!/usr/bin/env python3

import sys

for line in sys.stdin:
    line = line.replace("â˜€", "îŒ")
    line = line.replace("â˜ï¸", "ï™ž")
    line = line.replace("â›…ï¸", "îŒ‚")
    line = line.replace("â›ˆ", "îŒ")
    line = line.replace("âœ¨", "ï††")
    line = line.replace("ðŸŒ«", "îŒ“")
    line = line.replace("ðŸŒ©","ïª’")
    line = line.replace("â„ï¸", "ï‹œ")
    line = line.replace("ðŸŒ¨", "îž")
    line = line.replace("ðŸŒ§", "îŒ˜")
    line = line.replace("ðŸŒ¦", "îŒ‰")
    if len(sys.argv) > 2 and sys.argv[1] == "--color":
        print("%%{F%s}%s %%{F-} %s" % (sys.argv[2], line[0], line[3:]))
    else:
        print(line)

